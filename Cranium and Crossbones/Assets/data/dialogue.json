   "DialogTriggerID": "DT_03",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0018",
   "SpeakerName": "npc_04",
   "SpriteLeft": "",
   "SpriteRight1": "",
   "SpriteRight2": "",
   "SpriteRight3": "Left",
   "Dialogue": "Arr' you must be Ben Hornigold, Blackbeard told me to find you around these parts.",
