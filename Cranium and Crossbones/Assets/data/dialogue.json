   "audioClip": ""
 },
 {
   "DialogueID": "D_0021",
   "SpeakerName": "npc_02",
   "SpriteLeft": "",
   "SpriteRight1": "",
   "SpriteRight2": "",
   "SpriteRight3": "Right",
   "Dialogue": "Wonderful! Well your journey isnt over just yet. We need this Island fort cleared as well, Alas, the navy have caught wind of what happened and have already up their defenses against pirates like us.",
   "NextLine": "D_0022",
   "DialogTriggerID": "",
   "audioClip": ""
