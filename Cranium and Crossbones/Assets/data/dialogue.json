   "audioClip": ""
 },
 {
   "DialogueID": "D_0020",
   "SpeakerName": "npc_04",
   "SpriteLeft": "",
   "SpriteRight1": "",
   "SpriteRight2": "",
   "SpriteRight3": "Left",
   "Dialogue": "Aye that be true.",
