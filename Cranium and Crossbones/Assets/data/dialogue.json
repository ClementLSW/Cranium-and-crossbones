[
 {
   "DialogueID": "D_0001",
   "SpeakerName": "Edward \"Blackbeard\" Teach",
   "SpriteLeft": "Char_MainChar_Dark",
   "SpriteRight1": "Char_BlackBeard_1",
   "SpriteRight2": "",
   "SpriteRight3": "",
   "Dialogue": "Ahoy, Blackbeard! Fancy running into you on these treacherous seas",
   "NextLine": "D_0002",
   "DialogTriggerID": "DT_01",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0002",
   "SpeakerName": "Edward \"Blackbeard\" Teach",
   "SpriteLeft": "Char_MainChar_Dark",
   "SpriteRight1": "Char_BlackBeard_2",
   "SpriteRight2": "",
   "SpriteRight3": "",
   "Dialogue": "Congrats on stealing your first ship! She ain't much AND she ain't honest but more importantly she's yours!",
   "NextLine": "D_0003",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0003",
   "SpeakerName": "You",
   "SpriteLeft": "Char_MainChar_1",
   "SpriteRight1": "Char_BlackBeard_Dark",
   "SpriteRight2": "",
   "SpriteRight3": "",
   "Dialogue": "So what do i do now?",
   "NextLine": "D_0004",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0004",
   "SpeakerName": "Edward \"Blackbeard\" Teach",
   "SpriteLeft": "Char_MainChar_Dark",
   "SpriteRight1": "Char_BlackBeard_1",
   "SpriteRight2": "",
   "SpriteRight3": "",
   "Dialogue": "What else does a pirate do? SAIL, PLUNDER, DRINK AND MAKE MERRY! No time like right now!",
   "NextLine": "D_0005",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0005",
   "SpeakerName": "Edward \"Blackbeard\" Teach",
   "SpriteLeft": "Char_MainChar_Dark",
   "SpriteRight1": "Char_BlackBeard_2",
   "SpriteRight2": "",
   "SpriteRight3": "",
   "Dialogue": " 'Tis a fine day for piracy. I hear whispers of untamed islands ripe for the taking.",
   "NextLine": "D_0006",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0006",
   "SpeakerName": "Edward \"Blackbeard\" Teach",
   "SpriteLeft": "Char_MainChar_Dark",
   "SpriteRight1": "Char_BlackBeard_1",
   "SpriteRight2": "",
   "SpriteRight3": "",
   "Dialogue": "I have set my eyes on THREE islands out in the horizon due North East and West. These islands would definitely prove to be most valuable to make port and expand our pirate legacy. Ye want in?",
   "NextLine": "D_0007",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0007",
   "SpeakerName": "You",
   "SpriteLeft": "Char_MainChar_1",
   "SpriteRight1": "Char_BlackBeard_Dark",
   "SpriteRight2": "",
   "SpriteRight3": "",
   "Dialogue": "Ar' count me in. You know I can't turn down a challenge such as this arhahaha.",
   "NextLine": "D_0008",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0008",
   "SpeakerName": "Edward \"Blackbeard\" Teach",
   "SpriteLeft": "Char_MainChar_Dark",
   "SpriteRight1": "Char_BlackBeard_1",
   "SpriteRight2": "",
   "SpriteRight3": "Right",
   "Dialogue": "Well ye best be on your way! the islands have a healthy flow of merchant ships about. Sink them to get their resources, they wont pose much of a threat.",
   "NextLine": "D_0009",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0009",
   "SpeakerName": "You",
   "SpriteLeft": "Char_MainChar_1",
   "SpriteRight1": "Char_BlackBeard_Dark",
   "SpriteRight2": "",
   "SpriteRight3": "Left",
   "Dialogue": "Sounds like an breeze in the wind Aye",
   "NextLine": "D_0010",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0010",
   "SpeakerName": "Edward \"Blackbeard\" Teach",
   "SpriteLeft": "Char_MainChar_Dark",
   "SpriteRight1": "Char_BlackBeard_1",
   "SpriteRight2": "",
   "SpriteRight3": "Right",
   "Dialogue": "Beware young skipper, the more merchants you attack, the navy of the island will start to pursue you! Get rid of them and the fort on the Island and the rewards are yours to reap.",
   "NextLine": "D_0011",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0011",
   "SpeakerName": "You",
   "SpriteLeft": "Char_MainChar_1",
   "SpriteRight1": "Char_BlackBeard_Dark",
   "SpriteRight2": "",
   "SpriteRight3": "Left",
   "Dialogue": "Well I better get to it then.",
   "NextLine": "D_0012",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0012",
   "SpeakerName": "Edward \"Blackbeard\" Teach",
   "SpriteLeft": "Char_MainChar_Dark",
   "SpriteRight1": "Char_BlackBeard_1",
   "SpriteRight2": "",
   "SpriteRight3": "Right",
   "Dialogue": "I await your return after your first island takeover! Muahahah Good luck!",
   "NextLine": "",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0013",
   "SpeakerName": "Edward \"Blackbeard\" Teach",
   "SpriteLeft": "Char_MainChar_Dark",
   "SpriteRight1": "",
   "SpriteRight2": "",
   "SpriteRight3": "Right",
   "Dialogue": "Ahahaha! I knew you've got what it takes! The island is now ours!",
   "NextLine": "D_0014",
   "DialogTriggerID": "DT_02",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0014",
   "SpeakerName": "npc_04",
   "SpriteLeft": "",
   "SpriteRight1": "",
   "SpriteRight2": "",
   "SpriteRight3": "Left",
   "Dialogue": "I wouldn't say it was a simple walk in the park",
   "NextLine": "D_0015",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0015",
   "SpeakerName": "npc_01",
   "SpriteLeft": "",
   "SpriteRight1": "",
   "SpriteRight2": "",
   "SpriteRight3": "Right",
   "Dialogue": "Whats important is that you did it!.",
   "NextLine": "D_0016",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0016",
   "SpeakerName": "npc_01",
   "SpriteLeft": "",
   "SpriteRight1": "",
   "SpriteRight2": "",
   "SpriteRight3": "Right",
   "Dialogue": "However, there are still two more Islands to conquer! Head to the next Island! Ben Hornigold will await you there!",
   "NextLine": "",
   "DialogTriggerID": "",
   "audioClip": ""
 },
 {
   "DialogueID": "D_0017",
   "SpeakerName": "npc_02",
   "SpriteLeft": "",
   "SpriteRight1": "",
   "SpriteRight2": "",
   "SpriteRight3": "Right",
   "Dialogue": "Oi! Over here ya scoundrel!",
