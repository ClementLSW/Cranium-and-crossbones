 },
 {
   "DialogueID": "D_0019",
   "SpeakerName": "npc_02",
   "SpriteLeft": "",
   "SpriteRight1": "",
   "SpriteRight2": "",
   "SpriteRight3": "Right",
   "Dialogue": "Why yes it is I! If Blackbeard sent ye, then you must be the one that cleared the first island im guessing.",
